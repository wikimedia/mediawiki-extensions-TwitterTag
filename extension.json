{
	"name": "Twitter Tag",
	"version": "1.2.0",
	"author": [
		"TyA",
		"sactage",
		"X-Wing Team @ Wikia",
		"Mikko Miettinen"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:TwitterTag",
	"descriptionmsg": "twitter-tag-desc",
	"license-name": "GPL-3.0-or-later",
	"type": "parserhook",
	"requires": {
		"MediaWiki": ">= 1.27.0"
	},
	"AutoloadClasses": {
		"TwitterTagController": "TwitterTagController.php"
	},
	"Hooks": {
		"ParserFirstCallInit": "TwitterTagController::onParserFirstCallInit"
	},
	"MessagesDirs": {
		"TwitterTag": [
			"i18n"
		]
	},
	"ResourceModules": {
		"ext.TwitterTag": {
			"scripts": [
				"scripts/twitter.min.js"
			],
			"targets": [
				"desktop",
				"mobile"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "TwitterTag"
	},
	"manifest_version": 1
}
